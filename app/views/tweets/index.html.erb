<h1> Tweet </h1>

<table>
  <tr>
    <td> <%= link_to 'Users', users_path %> <td> |
    <td> <%= link_to 'Hashtags', '' %> <td> |
  </tr>
</table>

<table>
  <tbody>
    <% @tweets.each do |tweet| %>
      <tr>
        <td> <%= tweet.content %> by <%= tweet.user.handle %> <td>
      </tr>
      <tr>
        <td> Like by  <%= tweet.likes.count%> users </td>

      </tr>
    <% end %>
  </tbody>
</table>